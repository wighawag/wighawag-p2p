<?xml version="1.0" encoding="utf-8" ?>
<project>
    <yoga-version>1</yoga-version>
    <compiler-version>2.10</compiler-version>

    <id>com.wighawag.p2p</id>
    <version>0.0.1</version>

    <main>Main</main>

    <sources>
        <source path="src"/>
    </sources>

    <tests framework="munit" path="test"/>

    <targets hxml="compile">
        <target name="swf">
            <param line="-swf-version 11"/>
        </target>
    </targets>


    <config-file template="nmml.template" output="project.nmml"/>

    <dependencies>
        <repository id="com.wighawag.report.Report" version="0.0.1"/>
        <haxelib name="nme" version="3.4.0"/>
        <haxelib name="msignal" version="1.0.0"/>
        <sourcezip url="https://github.com/jdonaldson/promhx/zipball/master">
            <sources>
                <source path="\^[^/]+/src$\"/>
            </sources>
        </sourcezip>
    </dependencies>
</project>
